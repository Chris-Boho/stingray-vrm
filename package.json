{
  "name": "stingray-vrm",
  "displayName": "stingray-vrm",
  "description": "handle and edit vrm files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.01.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "vrmEditor.editor",
        "displayName": "VRM Editor",
        "selector": [
          {
            "filenamePattern": "*.vrm"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "vrmEditor.openEditor",
        "title": "Open with VRM Editor"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .vrm",
          "command": "vrmEditor.openEditor",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "dev": "concurrently \"npm run watch:extension\" \"npm run dev:webview\"",
    "dev:webview": "vite",
    "watch:extension": "tsc -p . --watch",
    "build": "npm run build:extension && npm run build:webview",
    "build:extension": "tsc -p .",
    "build:webview": "vite build"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/vscode": "^1.01.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitejs/plugin-react": "^4.5.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.1",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.8",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@tailwindcss/vite": "^4.1.8",
    "@xyflow/react": "^12.6.4",
    "immer": "^10.1.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zustand": "^5.0.5"
  }
}
